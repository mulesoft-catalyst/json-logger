{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Definition for fields used in the logger message processor",
  "type": "object",
  "properties": {
    "priority": {
      "type": "string",
      "javaType": "org.mule.extension.jsonlogger.pojos.Priority",
      "enum": [
        "DEBUG",
        "INFO",
        "WARN",
        "ERROR"
      ],
      "sdk": {
        "default": "INFO",
        "summary": "Logger priority"
      }
    },
    "tracePoint": {
      "type": "string",
      "javaType": "org.mule.extension.jsonlogger.pojos.TracePoint",
      "enum": [
        "START",
        "BEFORE_TRANSFORM",
        "AFTER_TRANSFORM",
        "BEFORE_REQUEST",
        "AFTER_REQUEST",
        "FLOW",
        "END",
        "EXCEPTION"
      ],
      "sdk": {
        "default": "START",
        "summary": "Current processing stage"
      }
    },
    "message": {
      "type": "string",
      "sdk": {
        "example": "Add a log entry",
        "required": true,
        "summary": "Message to be logged"
      }
    },
    "content": {
      "type": "string",
      "javaType": "org.mule.runtime.api.metadata.TypedValue<java.lang.Object>",
      "sdk": {
        "default": "#[output application/json ---\n{\n \tpayload: payload,\n \tattributes: attributes\n}]",
        "summary": "Remove if no content should be logged",
        "displayName": "Content default expression",
        "isContent": true
      }
    }
  }
}